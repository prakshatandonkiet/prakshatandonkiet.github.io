#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    
    char ch;
    char a[50];
    char s1[100];
     
     scanf("%c",&ch);
     scanf("%s",a);
     scanf("\n");
     scanf("%[^\n]%*c",&s1);
     printf("%c\n%s\n%s",ch,a,s1);
     
    return 0;
    
